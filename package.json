{
  "name": "jsreport-xlsx",
  "version": "1.4.1",
  "description": "jsreport recipe rendering excels directly from open xml",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsreport/jsreport-xlsx"
  },
  "homepage": "https://github.com/jsreport/jsreport-xlsx",
  "keywords": [
    "jsreport"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel --presets es2015,stage-0 src --out-dir lib",
    "watch": "babel --watch --presets es2015,stage-0 src --out-dir lib",
    "lint": "standard src test",
    "test": "npm run build && cross-env DEBUG=jsreport mocha --compilers js:babel-register --timeout 10000",
    "prepublish": "in-publish && jsreport-studio-build && npm-run-all lint clean build || not-in-publish"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "dependencies": {
    "archiver": "2.0.0",
    "bluebird": "3.5.0",
    "file-type": "5.2.0",
    "get-stream": "3.0.0",
    "lodash": "4.17.4",
    "merge2": "1.1.0",
    "msexcel-builder-extended": "0.0.7",
    "pify": "3.0.0",
    "request": "2.81.0",
    "shortid": "2.2.8",
    "stream-to-array": "2.3.0",
    "uuid": "3.1.0",
    "xml2js": "0.4.17",
    "yauzl": "2.8.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "cross-env": "2.0.1",
    "eslint": "4.4.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-assets": "0.6.6",
    "jsreport-core": "1.3.2",
    "jsreport-handlebars": "1.1.2",
    "jsreport-studio-dev": "1.1.1",
    "jsreport-templates": "1.1.1",
    "mocha": "3.5.0",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "should": "11.2.1",
    "standard": "10.0.2",
    "xlsx": "0.11.1"
  },
  "author": "Jan Blaha",
  "license": "MIT",
  "files": [
    "lib",
    "studio",
    "static",
    "index.js",
    "jsreport.config.js"
  ]
}
